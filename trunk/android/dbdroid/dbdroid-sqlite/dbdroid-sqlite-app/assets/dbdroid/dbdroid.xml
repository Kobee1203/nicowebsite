<?xml version="1.0" encoding="UTF-8"?>
<dbdroid:dbdroid-configuration
	xmlns:dbdroid="http://www.nds.org/dbdroid" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.nds.org/dbdroid dbdroid.xsd">

	<dbdroid:dao class="org.nds.dbdroid.sqlite.DbDroidSQLite" />
	<dbdroid:dao class="org.nds.dbdroid.sqlite.test.TestDao" />
	<dbdroid:dao package="org.nds.dbdroid.sqlite.test.dao" />

	<dbdroid:properties>
		<dbdroid:property name="dbdroid.show_query" value="true" />
		<dbdroid:property name="dbdroid.generate">update</dbdroid:property>
	</dbdroid:properties>
</dbdroid:dbdroid-configuration>

