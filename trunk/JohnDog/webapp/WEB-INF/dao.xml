<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="rightsDAO" class="org.nds.johndog.hibernate.dao.impl.RightsDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

	<bean id="domainDAO" class="org.nds.johndog.hibernate.dao.impl.DomainDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

	<bean id="itemDAO" class="org.nds.johndog.hibernate.dao.impl.ItemDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

 	<bean id="itemPropertyDAO" class="org.nds.johndog.hibernate.dao.impl.ItemPropertyDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

	<bean id="propertyDAO" class="org.nds.johndog.hibernate.dao.impl.PropertyDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

	<bean id="propertyTypeDAO" class="org.nds.johndog.hibernate.dao.impl.PropertyTypeDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

	<bean id="userDAO" class="org.nds.johndog.hibernate.dao.impl.UserDAOImpl">
		<property name="sessionFactory">
  			<ref bean="sessionFactory" />
  		</property>
  	</bean>

</beans>